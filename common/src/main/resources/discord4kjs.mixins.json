{
  "required": true,
  "minVersion": "0.8",
  "package": "chiefarug.mods.discord4kjs.mixin",
  "compatibilityLevel": "JAVA_17",
  "verbose": true,
  "mixins": [
    // Injections
    "ExtraThreadsShutdownMixin",
    "SkipScriptLoadingMixin",

    // Annotations adders
    "annotations.hidefromjs.JDAImpl",
    "annotations.hidefromjs.IOUtil",
    "annotations.hidefromjs.OkHttpClient$Builder",
    "annotations.hidefromjs.Requester",
    "annotations.hidefromjs.SessionConfig",
    "annotations.hidefromjs.ShardingSessionConfig",
    "annotations.remapforjs.Member",
    "annotations.remapforjs.User",

    // Method adders
    "methods.Guild",
    "methods.User",
    "methods.Member",
    "methods.Channel"
  ],
  "injectors": {
    "defaultRequire": 1
  }
}